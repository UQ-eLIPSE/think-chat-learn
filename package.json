{
  "name": "moocchat",
  "version": "2.3.8",
  "description": "",
  "main": "build/server/main.js",
  "scripts": {
    "start": "node build/server/main.js",
    "seed": "node server/util/db_seed.js",
    "m2p": "node server/util/m2p.js",
    "test_compile": "tsc --project ./test",
    "test_run": "mocha \"test-build/test/**/*.js\"",
    "test": "npm run clean_tests && npm run test_compile && npm run test_run",
    "clean": "rimraf build",
    "build_admin": "cd ./admin && npm run build",
    "build_admin_dev": "cd ./admin && npm run build_dev",
    "build_client": "cd ./client && npm run build",
    "build_client_dev": "cd ./client && npm run build_dev",
    "build_server_main": "tsc --project ./server/tsconfig.json",
    "build_server_copy_static": "copyfiles \"./server/static/**\" ./build",
    "build_server": "npm run build_server_main && npm run build_server_copy_static",
    "build": "npm run clean && npm run build_client && npm run build_server && npm run build_admin",
    "build_dev": "npm run clean && npm run build_client_dev && npm run build_server && npm run build_admin_dev"
  },
  "author": "eLIPSE",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "jsonwebtoken": "^8.4.0",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.11",
    "moment": "^2.23.0",
    "mongodb": "^3.1.10",
    "mongojs": "2.3.0",
    "oauth-signature": "^1.5.0",
    "socket.io": "2.1.1",
    "vuejs-countdown": "^0.2.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/express": "^4.16.0",
    "@types/express-jwt": "0.0.40",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/jwt-decode": "^2.2.1",
    "@types/lodash": "^4.14.119",
    "@types/mime": "^2.0.0",
    "@types/mocha": "^5.2.5",
    "@types/mongodb": "^3.1.17",
    "@types/node": "^10.12.15",
    "@types/serve-static": "^1.13.2",
    "@types/socket.io": "^2.1.2",
    "@types/socket.io-client": "^1.4.32",
    "chai": "^3.5.0",
    "concat-files": "0.1.0",
    "copyfiles": "1.0.0",
    "mkdirp": "0.5.1",
    "mocha": "^2.5.3",
    "rimraf": "^2.5.3",
    "typescript": "^3.0.1",
    "typings": "^1.3.0"
  }
}
